<h1>Listing recipes</h1>
<table>
  <thead>
    <tr>
      <th>name</th>
      <th>id</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @countryy.each do |country| %>
  
      <tr>
        <td><%= country.country_name %></td>
        <td><%= country.id %></td>
        <td><%= link_to 'Show', country %></td>
      
      </tr>
  
    <% end %>
  </tbody>
</table>

