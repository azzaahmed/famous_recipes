<h1>Listing recipes</h1>

<table>
  <thead>
    <tr>
      <th>name</th>
       <th>image</th>
      <th>id</th>
       <th>country_id</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @recipes.each do |recipe| %>
      <tr>
        <td><%= recipe.recipe_name %></td>

         
        <td><%= recipe.id %></td>
           <td><%= recipe.country_id %></td>
        <td><%= link_to 'Show', recipe %></td>
       
      
      </tr>
    <% end %>
  </tbody>
</table>

<br>


